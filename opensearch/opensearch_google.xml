<?xml version="1.0"?>
<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/">
	<ShortName>Google no-client</ShortName>
	<Description>Search Google</Description>
	<InputEncoding>UTF-8</InputEncoding>
	<LongName>Google Search (no client)</LongName>
	<Image height="16" width="16">data:image/png;base64,</Image>
	<Url type="text/html" template="https://www.google.com/search">
		<Param name="q" value="{searchTerms}" />
		<Param name="gbv" value="1" />
	</Url>
</OpenSearchDescription>
